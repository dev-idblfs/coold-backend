<script>
    $(document).ready(function () {
        $('#activeNewsFeed').submit(function (e) {
            e.preventDefault();
            e.stopPropagation();
            let data = $(this).serializeArray();
            $.ajax({
                type: 'POST',
                url: '<%- base_url -%>subcribeNewFeed',
                data: data,
                beforeSend: function () {
                    $('.preloader').show();
                },
                complete: function () {
                    $('.preloader').hide();
                    $('#activeNewsFeed').trigger('reset');
                },
                success: function (result) {
                    if (result.status == 200) {
                        swal({
                            text: "Thank you for Subscribe!",
                            icon: "success",
                            button: false,
                            timer: 1000
                        })
                    } else {
                        swal({
                            text: result.message,
                            icon: "error",
                            button: false,
                            timer: 1000
                        })
                    }
                },
                error: function (error) {
                    swal({
                        text: error.messageF,
                        icon: "error",
                        button: false,
                        timer: 1000
                    })
                }
            });
        })
    });
</script>

